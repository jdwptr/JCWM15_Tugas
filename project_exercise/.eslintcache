() {\r\n        Axios.get(url + key)\r\n            .then((response) => {\r\n                console.log(response)\r\n                this.setState({ berita: response.data.articles })\r\n            })\r\n            .catch((error) => console.log(error))\r\n    }\r\n\r\n    dropdownCountry = () => {\r\n        return (\r\n            <DropdownButton title='Country'>\r\n                <div>\r\n                    {this.state.listNeg.map((item, index) => {\r\n                        return (\r\n                            <Dropdown.Item key={index} onClick={() => this.pindahNeg(index)}>\r\n                                {item}\r\n                            </Dropdown.Item>\r\n                        )\r\n                    })}\r\n                </div>\r\n            </DropdownButton>\r\n\r\n        )\r\n    }\r\n\r\n    pindahNeg = (index) => {\r\n        url = this.state.negara[index]\r\n        Axios.get (url + key)\r\n            .then((response) => {\r\n                console.log(response.data)\r\n                this.setState ({berita: response.data.articles})\r\n            })\r\n            .catch((error) => console.log(error))\r\n        }\r\n\r\n    pindahCat = (index) => {\r\n        let newUrl = url.slice(0, 46) + this.state.catLink[index] + url.slice(46, url.length)\r\n        Axios.get (newUrl + key)\r\n            .then((response) => {\r\n                console.log(response.data)\r\n                this.setState ({berita: response.data.articles})\r\n            })\r\n            .catch((error) => console.log(error))\r\n        }\r\n\r\n    dropdownCategory = () => {\r\n        return (\r\n            <DropdownButton title='Category'>\r\n                <div>\r\n                    {this.state.category.map((item, index) => {\r\n                        return (\r\n                            <Dropdown.Item key={index} onClick={() => this.pindahCat(index)}>\r\n                                {item}\r\n                            </Dropdown.Item>\r\n                        )\r\n                    })}\r\n                </div>\r\n            </DropdownButton>\r\n\r\n        )\r\n    }\r\n\r\n    showCardNews = () => {\r\n        return this.state.berita.map((item, index) => {\r\n            return (\r\n                <Card style={{ width: '18rem', marginRight: '25px' }} key={index}>\r\n                    <Card.Img variant=\"top\" src={item.urlToImage} />\r\n                    <Card.Body>\r\n                        <Card.Title>{item.title}</Card.Title>\r\n                        <Card.Text>\r\n                            {item.description}\r\n                        </Card.Text>\r\n                        <Button href={item.url} variant=\"primary\" >Read more</Button>\r\n                    </Card.Body>\r\n                </Card>\r\n            )\r\n        })\r\n    }\r\n\r\n\r\n    render() {\r\n        console.log(this.state.berita)\r\n        return (\r\n            <div>\r\n                <div style={{ display: 'flex'}}>\r\n                    <h1>News API</h1>\r\n                    <div>{this.dropdownCountry()}</div>\r\n                    <div>{this.dropdownCategory()}</div>\r\n                </div>\r\n                <div style={{ display: 'flex', flexWrap: 'wrap' }}>\r\n                    {this.showCardNews()}\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Berita1",["71","72"],"C:\\JANE\\PURWADHIKA\\FRONTEND\\JCWM15_Tugas\\project_exercise\\src\\pages\\tablejson.js",[],"C:\\JANE\\PURWADHIKA\\FRONTEND\\JCWM15_Tugas\\project_exercise\\src\\index.js",[],"C:\\JANE\\PURWADHIKA\\FRONTEND\\JCWM15_Tugas\\project_exercise\\src\\pages\\linkpage.js",["73"],"import React from 'react'\r\nimport Home from './home'\r\nimport {Link} from 'react-router-dom'\r\n\r\nimport {\r\n    Button\r\n} from 'react-bootstrap'\r\n\r\nclass Linkpage extends React.Component {\r\n    render () {\r\n        return (\r\n            <div style={style.container}>\r\n                <h